{
  "name": "sld-viewer-server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "build": "cd client && npm ci && npm run build",
    "postinstall": "npm run build",
    "prestart": "prisma generate --schema=./server/prisma/schema.prisma",
    "start": "node server/server.js",
    "seed": "node prisma/seed.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "pg": "^8.16.3",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "prisma": "^7.0.0",
    "vite": "^7.1.3"
  }
}
